    "invoked": "<%= lifecycle %>",
    "results": [
        <% last = results.pop %>

        <% results.each do |result| %>
        {
            <% i = 1 %>
            <% result.each do |key, val| %>"<%= key %>": "<%= val %>"<% if i < result.length %>,
            <% i = i + 1 %>
            <% end %>
            <% end %>
        },
        <% end %>
        <% if last %>
        {
            <% i = 1 %>
            <% last.each do |key, val| %>"<%= key %>": "<%= val %>"<% if i < last.length %>,
            <% i = i + 1 %>
            <% end %>
            <% end %>
        }
        <% end %>
    ]
