    "invoked": "<%= lifecycle %>",
    "results": [
        <% last = results.pop %>

        <% results.each do |result| %>
        {
            <% result.each do |key, val| %>"<%= key %>": "<%= val %>" <% end %>
        },
        <% end %>
        <% if last %>
        {
            <% last.each do |key, val| %>"<%= key %>": "<%= val %>" <% end %>
        }
        <% end %>
    ]
